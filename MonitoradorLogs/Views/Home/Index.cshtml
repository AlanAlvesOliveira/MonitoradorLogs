@model IEnumerable<Log>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Logs</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
</head>
<body>
    <div class="container">
        <h1 class="mt-4">Logs</h1>
        <p>Última atualização: @ViewBag.ult_atualizacao</p>
        <table class="table table-bordered table-striped mt-4">
            <thead class="thead-dark">
                <tr>
                    <th>Id</th>
                    <th>Hora Inserção</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var log in Model)
                {
                    <tr>
                        <td>@log.Id</td>
                        <td>@log.HoraInsercao</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
